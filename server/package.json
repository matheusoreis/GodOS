{
    "name": "server",
    "version": "0.0.1",
    "description": "",
    "author": "Matheus R. Oliveira",
    "license": "Apache-2.0",
    "type": "module",
    "keywords": [],
    "scripts": {
        "dev": "tsx watch --env-file .env src/main.ts",
        "build": "tsc",
        "start": "node --env-file .env dist/main.js",
        "format": "npx prettier . --write",
        "sqlite:up": "tsx --env-file .env --no-warnings ./node_modules/knex/bin/cli.js --knexfile ./src/database/knexfile.ts --env sqlite migrate:latest",
        "sqlite:make": "tsx --env-file .env --no-warnings ./node_modules/knex/bin/cli.js --knexfile ./src/database/knexfile.ts --env sqlite migrate:make",
        "postgres:up": "tsx --env-file .env --no-warnings ./node_modules/knex/bin/cli.js --knexfile ./src/database/knexfile.ts --env postgres migrate:latest",
        "postgres:make": "tsx --env-file .env --no-warnings ./node_modules/knex/bin/cli.js --knexfile ./src/database/knexfile.ts --env postgres migrate:make"
    },
    "dependencies": {
        "bcrypt": "^6.0.0",
        "knex": "^3.1.0",
        "pg": "^8.16.3",
        "sqlite3": "^5.1.7",
        "ws": "^8.18.3"
    },
    "devDependencies": {
        "@types/bcrypt": "^6.0.0",
        "@types/node": "^24.3.1",
        "@types/ws": "^8.18.1",
        "prettier": "3.6.2",
        "tsx": "^4.20.5",
        "typescript": "^5.9.2"
    }
}
